make
	#define the data
	data "out.dat"
	read {xgas 1 ygas 2 egas 3 xstar 4 ystar 5 estar 6}

	#set the plot area
	device postencap "out.eps"
	erase
	window 1 1 1 1
	#limits x y
	limits 0 10 0 1
	ticksize 1 1 0.1 0.1
	ctype black
	box

	#plot the data: gas
	ctype red
	ltype 2
	connect xgas ygas
	ltype 0
	error_y xgas ygas egas

	#stars
	ctype green
	ltype 4
	connect xstar ystar
	ltype 0
	error_y xstar ystar estar

	#labels
	define TeX_strings 1
	expand 1.001
	ctype black
	#toplabel Example 1
	xlabel r/r_{vir,0}
	ylabel \phi(j_{dm},j_{gas,star})
	#ctype red
	#relocate 8 2
	#draw 9 2
	#relocate 8.5 2
	#dot
	#relocate 8 2.25
	#ctype black
	#label y=x^{2/3}

