make
	#define the data
	data "out.dat"
	read {xdm 1 ydm 2 edm 3 xgas 4 ygas 5 egas 6 xstar 7 ystar 8 estar 9}

	#set the plot area
	device postencap "out.eps"
	erase
	window 1 1 1 1
	#limits x y
	limits -1 1.5 -3 0
	ticksize -1 10 -1 10
	ctype black
	box

	#dm
	ctype blue
	ltype 0
	connect xdm ydm
	error_y xdm ydm edm

	#gas
	ctype red
	ltype 0
	connect xgas ygas
	error_y xgas ygas egas

	#star
	ctype green
	ltype 0
	connect xstar ystar
	error_y xstar ystar estar

	#labels
	define TeX_strings 1
	expand 1.001
	ctype black
	#toplabel Example 1
	xlabel \Sigma/V
	ylabel p(\Sigma/V)
	#ctype red
	#relocate 8 2
	#draw 9 2
	#relocate 8.5 2
	#dot
	#relocate 8 2.25
	#ctype black
	#label y=x^{2/3}

