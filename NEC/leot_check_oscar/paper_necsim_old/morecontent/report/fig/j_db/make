make
	#define the data
	data "out.dat"
	read {x 1 y 2 e 3}

	#set the plot area
	device postencap "out.eps"
	erase
	window 1 1 1 1
	#limits x y
	limits -1 1 0 0.25
	ticksize 0.2 0.2 0.05 0.05
	ctype black
	box

	#plot the data
	ctype red
	ltype 0
	connect x y
	error_y x y e

	#labels
	define TeX_strings 1
	expand 1.001
	ctype black
	#toplabel Example 1
	xlabel \phi(j_{dm},j_{b})
	ylabel p(\phi)
	#ctype red
	#relocate 8 2
	#draw 9 2
	#relocate 8.5 2
	#dot
	#relocate 8 2.25
	#ctype black
	#label y=x^{2/3}

